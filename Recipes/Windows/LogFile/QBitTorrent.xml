   <LogFile>

        <Source>qBittorrent</Source>
        <PathAndMask>C:/Users/Administrator/AppData/Local/qBittorrent/logs/qbittorrent.log</PathAndMask>
        <Recursive>true</Recursive>
        <FailedLoginRegex>
          <![CDATA[
              \s(?<timestamp>[^\s]+)\s-\sWebAPI\slogin\sfailure.*IP:\s(?<ipaddress>[^\s]+),\susername:\s(?<username>[^\n]+)
          ]]>
        </FailedLoginRegex>
        <SuccessfulLoginRegex>
          <![CDATA[
              \s(?<timestamp>[^\s]+)\s-\sWebAPI\slogin\ssuccess\.\sIP:\s(?<ipaddress>[^\s]+)
          ]]>
        </SuccessfulLoginRegex>
        <PlatformRegex>Windows</PlatformRegex>
        <PingInterval>10000</PingInterval>
        <MaxFileSize>16777216</MaxFileSize>
        <FailedLoginThreshold>0</FailedLoginThreshold>

    </LogFile>
